    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "\r\n SigninLogs\r\n| extend SigninType = iff(Status contains \"failurereason\",  \"Interactive Failure\", \"Interactive Success\") \r\n| summarize InteractiveSuccess = countif(SigninType contains \"Interactive Success\"), InteractiveFailure = countif(SigninType contains \"Interactive Failure\"), TotalInteractiveSignIns = count(SigninType)\r\n| project [\"Total Interactive SignIns\"] = TotalInteractiveSignIns, [\"Successful Interactive SignIns\"] = InteractiveSuccess, [\"Failed Interactive SignIns\"] = InteractiveFailure \r\n",
        "size": 3,
        "title": "Summary of Interactive Sign-Ins",
        "timeContext": {
          "durationMs": 2592000000
