        "version": "KqlItem/1.0",
        "query": "//Non-Interactive signins by country and city\r\nlet data = AADNonInteractiveUserSignInLogs\r\n    | extend AppDisplayName = iff(AppDisplayName == '', 'Unknown', AppDisplayName)\r\n    | where AppDisplayName in ('*') or '*' in ('*')\r\n    | where UserDisplayName in ('*') or '*' in ('*')\r\n    | extend SigninType = iff(Status contains \"failurereason\", \"Non-Interactive Failure\", \"Non-Interactive Success\") \r\n    | extend Location = todynamic(LocationDetails)\r\n    | extend Country = iff(Location[\"countryOrRegion\"] == '', 'Unknown', Location[\"countryOrRegion\"])\r\n    | extend City = iff(Location[\"city\"] == '', 'Unknown city', Location[\"city\"])\r\n    | extend ParsedFields = parse_json(Status)\r\n    | extend ErrorCode = ParsedFields.errorCode;\r\nlet countryData = data\r\n    | summarize NonInteractiveTotalCount = count(), NonInteractiveSuccessCount = countif(SigninType == \"Non-Interactive Success\"), NonInteractiveFailureCount = countif(SigninType == \"Non-Interactive Failure\") by Country\r\n    | join kind=inner\r\n        (\r\n        data\r\n        | make-series Trend = count() default = 0 on TimeGenerated in range(ago(30d), now(), 1d) by Country\r\n        | project-away TimeGenerated\r\n        )\r\n        on Country\r\n    | project Country, NonInteractiveTotalCount, NonInteractiveSuccessCount, NonInteractiveFailureCount, Trend\r\n    | order by NonInteractiveTotalCount desc, Country asc;\r\ndata\r\n| summarize NonInteractiveTotalCount = count(), NonInteractiveSuccessCount = countif(SigninType == \"Non-Interactive Success\"), NonInteractiveFailureCount = countif(SigninType == \"Non-Interactive Failure\") by Country, City\r\n| join kind=inner\r\n    (\r\n    data    \r\n    | make-series Trend = count() default = 0 on TimeGenerated in range(ago(30d), now(), 1d) by Country, City\r\n    | project-away TimeGenerated\r\n    )\r\n    on Country, City\r\n| order by NonInteractiveTotalCount desc, Country asc\r\n| project Country, City, NonInteractiveTotalCount, NonInteractiveFailureCount, NonInteractiveSuccessCount, Trend\r\n| join kind=inner \r\n    (\r\n    countryData\r\n    )\r\n    on Country\r\n| project Id = City, Name = City, Type = 'City', NonInteractiveTotalCount, Trend, NonInteractiveFailureCount, NonInteractiveSuccessCount, ['Non-Interactive Success Rate'] = 1.0 * NonInteractiveSuccessCount / NonInteractiveTotalCount, ParentId = Country\r\n| union (\r\n    countryData\r\n    | project Id = Country, Name = Country, Type = 'Country', NonInteractiveTotalCount, Trend, NonInteractiveFailureCount, NonInteractiveSuccessCount, ['Non-Interactive Success Rate'] = 1.0 * NonInteractiveSuccessCount / NonInteractiveTotalCount, ParentId = 'root')\r\n| order by NonInteractiveTotalCount desc, Name asc ",
