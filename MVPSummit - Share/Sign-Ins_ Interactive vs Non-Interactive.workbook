      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "SigninLogs\r\n| union (AADNonInteractiveUserSignInLogs)\r\n| summarize count() by Category\r\n",
        "size": 3,
        "title": "Interactive vs. Non-Interactive Sign-ins",
