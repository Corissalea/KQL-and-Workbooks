    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "\r\n AADNonInteractiveUserSignInLogs\r\n   | extend SigninType = iff(Status contains \"failurereason\", \"Non-Interactive Failure\", \"Non-Interactive Success\")\r\n   | summarize NonInteractiveSuccess = countif(SigninType contains \"Non-Interactive Success\"), NonInteractiveFailure = countif(SigninType contains \"Non-Interactive Failure\"), TotalNonInteractiveSignIns = count(SigninType)\r\n   |project [\"Total Non-Interactive SignIns\"] = TotalNonInteractiveSignIns, [\"Successful Non-Interactive SignIns\"] = NonInteractiveSuccess, [\"Failed Interactive SignIns\"] = NonInteractiveFailure",
        "size": 3,
        "title": "Summary of Non-Interactive Sign-Ins",
